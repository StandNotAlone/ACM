int main()
{
	ios::sync_with_stdio(false);
	cin.tie(0);
	int n;
	cin>>n;
	while(n--)
	{
		int h,w;
		cin>>h>>w;
		h-=100;
		w*=50;
		if(w>=99*h) cout<<"You are tai pang le!"<<endl;
		else if(w<=81*h) cout<<"You are tai shou le!"<<endl;
		else cout<<"You are wan mei!"<<endl;
	}
}


