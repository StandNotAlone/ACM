const ll maxn=6e5+7;
const double eps=1e-10;

int main()
{
    ios::sync_with_stdio(false);
    cin.tie(0);
    char c;
    int n;
    cin>>c>>n;
    vector<string>F1(n),F2(n);
    cin.get();
    for(auto &x:F1)
        getline(cin,x);
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<n;j++)
        {
            if(F1[i][n-1-j]!=' ')
            {
                F2[n-1-i].pb(c);
            }
            else F2[n-1-i].pb(' ');
        }
    }
    bool f=1;
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<n;j++)
        {
            if(F1[i][j]!=' '&&F2[i][j]==' '||F1[i][j]==' '&&F2[i][j]!=' ')
            {
                f=0;
                break;
            }
        }
    }
    if(f) cout<<"bu yong dao le"<<endl;
    for(auto &x:F2)
    {
        cout<<x<<endl;
    }
}


