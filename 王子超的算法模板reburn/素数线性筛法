vector<ll>prime;
ll v[maxn];//保存下标值的最小质因子用于线性筛的实现

void primes(ll n)
{
    for(ll i=2;i<=n;i++)
    {
        if(!v[i])//v[i]为0代表0为质数
        {
            v[i]=i;
            prime.pb(i);
        }
        for(ll j=0;j<prime.size();j++)
        {
            if(prime[j]>v[i]||prime[j]*i>n) break;//如果当前找寻的质数大于i的最小质因数或者相乘后超出n的范围则停止
            v[prime[j]*i]=prime[j];
        }
    }
}