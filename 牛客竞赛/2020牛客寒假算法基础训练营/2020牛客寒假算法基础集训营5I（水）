const int maxn=INF;
int main()
{
    ios::sync_with_stdio(false);
    cin.tie(0);
    ll n,m,a[20];
    cin>>n>>m;
    for(ll i=1;i<=n;i++)
        cin>>a[i];
    ll x=m*4/5;
    if(m*4/5*5!=m*4) x++;
    ll flag=0;
    for(ll i=1;i<=n;i++)
        if(a[i]>a[9]) flag++;
    if(flag<3||a[9]>=x) cout<<"Yes"<<endl;
    else cout<<"No"<<endl;
}



